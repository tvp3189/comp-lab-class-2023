restart 10000000 prod/kalj_T0.5_n360_v300_prod_10000000_1.final.lmprestart

thermo ${thermotime}
thermo 100000
timestep ${timestep}
timestep 0.003

neighbor 0.5 bin
neigh_modify every 1 delay ${neighborstep} check yes
